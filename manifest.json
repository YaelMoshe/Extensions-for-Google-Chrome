{
  "manifest_version": 2,

  "name": "Calculator",
  "description": "Calculator",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  
  "browser_action": {
   "default_icon": "ToolbarIcon.png",
   "default_popup": "popup.html"
  },

  "permissions": [
   "activeTab"
   ]
}